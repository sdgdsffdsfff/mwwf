<?xml version="1.0" encoding="UTF-8"?>
<flows xmlns="http://www.suning.com/schema/services/flow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.suning.com/schema/services/flow flow.xsd">
	<flow name="kafka" autoTrigger="true">
		<stage name="firstStage" toStage="secondStage">
			<router name="A" eventName="B" toStage="secondStage"/>
		</stage>
		<stage name="secondStage">
			<router name="A" eventName="B" toStage="endStage"/>
			<router name="B" eventName="B" toStage="secondStage"/>
		</stage>
		<stage name="endStage"/>
	</flow>
</flows>
